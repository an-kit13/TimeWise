{
    "manifest_version": 3,
    "name": "TimeWise",
    "version": "1.0",
    "description": "Tracks screen time and categorizes websites.",
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon128.png"
      }
    },
    "icons": {
      "128": "icon128.png"
    }
  }
  